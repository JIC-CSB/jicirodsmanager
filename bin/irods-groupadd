#!/bin/bash

MYPATH=`realpath $0`
BINPATH=`dirname $MYPATH`
PACKAGEPATH=`dirname $BINPATH`
export PYTHONPATH=$PYTHONPATH:$PACKAGEPATH

if [ `whoami` == "irods" ]
then
        python $PACKAGEPATH/jicirodsmanager/cli.py addgroup $@
else
        su irods -c "python $PACKAGEPATH/jicirodsmanager/cli.py addgroup $@"
fi
